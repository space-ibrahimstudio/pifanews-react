{
  "name": "pifanews-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ibrahimstudio/button": "^1.2.0",
    "@ibrahimstudio/icons": "^1.0.7",
    "@ibrahimstudio/input": "^1.3.7",
    "@ibrahimstudio/react": "^1.0.2",
    "axios": "^1.6.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "start": "react-scripts start",
    "build": "npm run clean && cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "clean": "npm run backup && rimraf build",
    "eject": "react-scripts eject",
    "deploy": "cross-env GENERATE_SOURCEMAP=false react-scripts build && react-snap",
    "static": "npm run build && react-snap",
    "backup": "rimraf backup && node src/libs/plugins/backup.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "cross-env": "^7.0.3",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "crawl": true,
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true,
      "removeEmptyAttributes": true
    }
  }
}
