{
  "name": "pifanews-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ibrahimstudio/button": "^1.3.0",
    "@ibrahimstudio/icons": "^1.0.8",
    "@ibrahimstudio/input": "^1.4.5",
    "@ibrahimstudio/react": "^1.0.3",
    "axios": "^1.6.8",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "xml2js": "^0.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "backup": "rimraf backup && node src/libs/plugins/backup.js",
    "rstore": "node src/libs/plugins/restore.js",
    "clean": "npm run backup && rimraf build",
    "crawl": "node update-slugs.js",
    "spagen": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "stagen": "npm run crawl && npm run spagen && react-snap"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "cross-env": "^7.0.3",
    "puppeteer": "^22.13.1",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.5",
    "shelljs": "^0.8.5"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "crawl": true,
    "include": [
      "/",
      "/login"
    ],
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true,
      "removeEmptyAttributes": true
    },
    "puppeteer": {
      "timeout": 60000
    }
  }
}
